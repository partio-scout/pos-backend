{
  "name": "pos-backend",
  "version": "0.1.0",
  "description": "Partio-ohjelmasovelluksen backend",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "ngrok": "ngrok http -subdomain=pos-api-dev 3001",
    "build": "rimraf dist && babel src --out-dir dist",
    "migrate": "node-pg-migrate",
    "migrate-up": "node-pg-migrate up",
    "migrate-down": "node-pg-migrate down"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "node-pg-migrate": "3.23.3",
    "passport": "^0.4.1",
    "passport-saml": "1.2.0",
    "passport-saml-metadata": "2.1.0",
    "pg": "7.13.0",
    "pg-promise": "10.2.1",
    "rimraf": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "eslint": "6.6.0",
    "husky": "3.1.0",
    "nodemon": "1.19.4",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
